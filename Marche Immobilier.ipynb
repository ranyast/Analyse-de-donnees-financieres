    "# Stocker les sÃ©lections de l'utilisateur\n",
    "selections = {name: [] for name in urls.keys()}\n",
    "\n",
    "# Fonction de style dynamique\n",
    "def style_callback_factory(level):\n",
    "    def style_callback(feature):\n",
    "        nom = feature[\"properties\"].get(\"nom\", feature[\"properties\"].get(\"libelle\", \"â€”\"))\n",
    "        if nom in selections[level]:\n",
    "            return {\"fillColor\": \"green\", \"color\": \"black\", \"weight\": 1, \"fillOpacity\": 0.6}\n",
    "        else:\n",
    "            return {\"fillColor\": \"lightblue\", \"color\": \"black\", \"weight\": 1, \"fillOpacity\": 0.3}\n",
    "    return style_callback\n",
    "\n",
    "# Carte centrÃ©e sur la France\n",
    "m = Map(center=(46.5, 2.5), zoom=5)\n",
    "\n",
    "# Callback commun\n",
    "def make_click_handler(level, layer):\n",
    "    def handle_click(event, feature, **kwargs):\n",
    "        nom = feature[\"properties\"].get(\"nom\", feature[\"properties\"].get(\"libelle\", \"â€”\"))\n",
    "        if nom in selections[level]:\n",
    "            selections[level].remove(nom)\n",
    "            print(f\"âŒ {level} retirÃ© : {nom}\")\n",
    "        else:\n",
    "            selections[level].append(nom)\n",
    "            print(f\"âœ… {level} ajoutÃ© : {nom}\")\n",
    "        layer.style_callback = style_callback_factory(level)  # mettre Ã  jour le style\n",
    "    return handle_click\n",
    "\n",
    "# CrÃ©er et ajouter les couches\n",
    "for level, data in geo_data.items():\n",
    "    layer = GeoJSON(\n",
    "        data=data,\n",
    "        style_callback=style_callback_factory(level),\n",
    "        hover_style={\"fillColor\": \"yellow\"},\n",
    "        name=level\n",
    "    )\n",
    "    layer.on_click(make_click_handler(level, layer))\n",
    "    m.add(layer)\n",
    "\n",
    "# Ajouter un contrÃ´le de couches\n",
    "m.add(LayersControl(position=\"topright\"))\n",
    "\n",
    "m\n"
